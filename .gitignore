# Wordpress - ignore core, configuration, examples, uploads and logs.
# https://github.com/github/gitignore/blob/main/WordPress.gitignore

# IDE Files
/.idea
/local-xdebuginfo.php

# Core
#
# Note: if you want to stage/commit WP core files
# you can delete this whole section/until Configuration.
/wp-admin/
/wp-content/index.php
/wp-content/languages
/wp-content/plugins/index.php
/wp-content/themes/index.php
/wp-includes/
/index.php
/license.txt
/readme.html
/wp-*.php
/xmlrpc.php

# Configuration
wp-config.php

# Example themes
#/wp-content/themes/

# Example plugin
/wp-content/plugins/hello.php

# Uploads
/wp-content/uploads/

# Log files
*.log

# htaccess
/.htaccess

# All "wp-content" folder content, but keep the wp-content folder
!/wp-content
/wp-content/*

#
# ACF Files
#
!/wp-content/acf-jsons

#
# Plugins
#
!/wp-content/plugins
/wp-content/plugins/*

# Note: If you wish to whitelist plugins, add them below
!/wp-content/plugins/softuni-games-plg


# Ignore the themes in general
!/wp-content/themes
/wp-content/themes/*
#
# Note: If you wish to whitelist themes
!/wp-content/themes/softuni-games-thm
